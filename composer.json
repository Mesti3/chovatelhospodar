{
    "name": "misaon/nettpack-vue",
    "description": "Nette, Vue.js and Webpack 4 web project",
    "keywords": [
        "vue.js",
        "nette",
        "webpack"
    ],
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">= 7.1",
        "nette/application": "^3.0",
        "nette/bootstrap": "^3.0",
        "nette/caching": "^3.0",
        "nette/database": "^3.0",
        "nette/di": "^3.0",
        "nette/finder": "^3.0",
        "nette/forms": "^3.0",
        "nette/http": "^3.0",
        "nette/mail": "^3.0",
        "nette/robot-loader": "^3.0",
        "nette/security": "^3.0",
        "nette/utils": "^3.0",
        "latte/latte": "^3.0",
        "tracy/tracy": "^2.5",
        "justinrainbow/json-schema": "^5.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "nette/tester": "^2.0"
    },
    "minimum-stability": "dev",
    "config": {
        "platform": {
            "php": "7.1"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "composer update -o",
            "yarn install"
        ]
    }
}
